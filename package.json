{
  "name": "js-util-log",
  "version": "1.0.0",
  "description": "A simple and colorful logger abstraction.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "node .",
    "watch": "nodemon . -e js,jsx --ignore .build",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-register",
    "tdd": "npm run test -- --reporter min --watch",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.jsx ./src",
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir lib --source-maps",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm test && npm run lint && npm run build"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "ramda": "^0.21.0"
  },
  "devDependencies": {
    "js-babel": "^6.0.0",
    "js-babel-dev": "^6.0.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "github.com/philcockfield/js-util-log"
  },
  "keywords": [
    "log",
    "console"
  ],
  "author": {
    "name": "Phil Cockfield",
    "email": "phil@cockfield.net",
    "url": "github.com/philcockfield"
  },
  "homepage": "github.com/philcockfield/js-util-log",
  "license": "MIT"
}
