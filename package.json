{
  "name": "log.server",
  "version": "1.0.4",
  "description": "A simple and colorful logger abstraction.",
  "main": "./lib/log.js",
  "typings": "./lib/log.d.ts",
  "scripts": {
    "start": "node .",
    "watch": "nodemon . -e js,jsx --ignore .build src",
    "test": "./node_modules/mocha/bin/mocha --opts mocha.opts",
    "tdd": "npm run test -- --reporter min --watch",
    "lint": "node ./node_modules/tslint/bin/tslint 'src/**/*.ts{,x}' --format verbose",
    "build": "rm -rf ./lib && node ./node_modules/typescript/bin/tsc",
    "prepublish": "npm test && npm run lint && npm run build"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "js-yaml": "^3.6.1",
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chalk": "^0.4.30",
    "@types/js-yaml": "^3.5.28",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/ramda": "0.0.2",
    "chai": "^3.5.0",
    "js-typescript": "^2.1.0",
    "mocha": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "github.com/philcockfield/log.server"
  },
  "keywords": [],
  "author": {
    "name": "Phil Cockfield",
    "email": "phil@cockfield.net",
